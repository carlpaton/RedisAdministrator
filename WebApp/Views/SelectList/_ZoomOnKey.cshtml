@model ZoomViewModels

<div class="container">
    <div class="row">
        <label asp-for="Key" class="col-sm-2"></label>
        <div class="col-sm-10">
            @Html.Raw(Model.Key)
        </div>
    </div>
    @{if (Model.Score != null)
        {
            <div class="row">
                <label asp-for="Score" class="col-sm-2"></label>
                <div class="col-sm-10">
                    @Html.Raw(Model.Score)
                </div>
            </div>
        }
    }
    <div class="row">
        <label asp-for="Type" class="col-sm-2"></label>
        <div class="col-sm-10">
            @Html.Raw(Model.Type)
        </div>
    </div>
    <div class="row">
        <label asp-for="Value" class="col-sm-2"></label>
        <div class="col-sm-10">
            @Html.Raw(Model.Value)
        </div>
    </div>
</div>